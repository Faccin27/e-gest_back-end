datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Sexo {
  Masculino
  Feminino
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  sexo      Sexo     @default(Masculino)
  createdAt DateTime @default(now())
}
